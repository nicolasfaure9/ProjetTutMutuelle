{% extends "layout.html.twig" %}
{% set Mutuelle = true %}

{% block title %}Remboursement{% endblock %}

{% block content %}
    <div>
    <ol class="breadcrumb"  >
                    <li><a href="/accueil">Accueil</a></li>
                    <li><a href="/remboursements">Mes remboursements</a></li>       
    </ol>
    </div>
<h2 class="text-center">{{ block('title') }}</h2>
{% if prestation_sante %}
    <div class="table-responsive">
        <table  class="table table-hover table-condensed"> 
            <p class="form-control-static">{{ prestation_sante.designation_acte }}</p>
            <p class="form-control-static">{{ prestation_sante.date_paiement }}</p>
            <p class="form-control-static">{{ prestation_sante.date_soins }}</p>
        </table>
    </div>
{% else %}
    <div class="alert alert-warning">Erreur chargement de vos remboursements.</div>
{% endif %}   
{% endblock %}{# empty Twig template #}
